<%include header %>
		<h1><%= title %></h1>
		<% if(dividends){ %>
			<ui>
			<% dividends.forEach(function(dividend){ %>
				<li> <%= dividend.product %> : <%= dividend.winningSelections %> : $<%= dividend.amount %></li>
			<% }) %>
			</ui>
		<% } %>
		<br><br>
		<form method="GET" action="/races/<%= raceId %>""> 
			<input type="submit" value="Back to main page">
		</form>
<%include footer %>	